{
  "name": "@parsonic/copy-to-clipboard",
  "version": "0.0.0",
  "description": "Copy text to clipboard web component",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "custom-elements.json",
    "*.js",
    "*.map",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/p-m-p/parsonic"
  },
  "scripts": {
    "build": "node ./build.js",
    "dev": "node ./watch.js",
    "generate-manifest": "cem analyze --globs 'src/CopyToClipboard.js'",
    "clean": "rm -f index.js CopyToClipboard.js min.js min.map.js",
    "prepare": "cp ../../LICENSE ./"
  },
  "keywords": [
    "clipboard",
    "component",
    "copy",
    "links",
    "web"
  ],
  "author": "Phil Parsons",
  "license": "MIT",
  "customElements": "custom-elements.json",
  "devDependencies": {
    "@parsonic/esbuild-plugin": "workspace:*"
  }
}
