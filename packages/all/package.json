{
  "name": "@parsonic/all",
  "version": "1.1.3",
  "description": "All of the @parsonic web components in one package",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "*.js",
    "*.map",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/p-m-p/parsonic"
  },
  "scripts": {
    "build": "node ../../scripts/build.js",
    "lint": "eslint .",
    "types": "tsc",
    "prepare": "cp ../../LICENSE ./"
  },
  "keywords": [
    "component",
    "css",
    "html",
    "web"
  ],
  "author": "Phil Parsons",
  "license": "MIT",
  "dependencies": {
    "@parsonic/back-to-top": "workspace:*",
    "@parsonic/copy-to-clipboard": "workspace:*",
    "@parsonic/share-button": "workspace:*",
    "@parsonic/theme-switch": "workspace:*"
  }
}
